C,YAAI;AAAC,MAAA,KAAI,CAAC,IAAL,GAAU,KAAV;AAAgB,KAAtD;AACD,GAzBY;AA0Bb,EAAA,OAAO,EAAE;AACP,IAAA,UADO,wBACK;AACV,WAAK,IAAL,GAAY,CAAC,KAAK,IAAlB;AACA,WAAK,KAAL,CAAW,aAAX;AACD,KAJM;AAKP,IAAA,cALO,4BAKS;AACd,MAAA,MAAM,CAAC,QAAP,CAAgB,IAAhB,GAAuB,GAAvB;AACD,KAPM;AAQP,IAAA,gBARO,8BAQW;AAChB,UAAI,aAAY,GAAI,MAAM,CAAC,QAA3B;AACA,MAAA,MAAM,CAAC,QAAP,CAAgB,IAAhB,GAAuB,gCAA8B,aAArD;AACD,KAXM;AAYP,IAAA,MAZO,oBAYC;AACN,MAAA,KAAK,CAAC,GAAN,CAAU,SAAV;AACA,WAAK,KAAL,CAAW,QAAX,CAAoB,WAApB,EAAiC,KAAjC;AACA,WAAK,KAAL,CAAW,QAAX,CAAoB,aAApB,EAAmC,IAAnC;AACF;AAhBO;AA1BI,CAAf","sourcesContent":["<template>\n  <div class=\"user-hamburger-menu__main-panel\" @click.stop=\"\" :class=\"{'hidden': !show}\">\n    <div class=\"user-hamburger-menu__menu\">\n      <img class=\"home-button\" src=\"@/assets/logo.png\" @click=\"redirectToHome\">\n      <div class=\"logged-menu\" v-if=\"username\">\n        <div>\n          <p>Username: <strong>{{decodeURIComponent(username)}}</strong></p>\n          <button @click=\"logout\" v-if=\"!disableLogout\">Logout</button>\n        </div>\n        <button class=\"procede-with-google\" v-if=\"!logged\" @click=\"redirectToGoogle\">Activate sync with google</button>\n      </div>\n      <div class=\"not-logged-menu\" v-else>\n        <p>You are currently not logged in</p>\n        <button class=\"procede-with-google\" @click=\"redirectToGoogle\">Sign in now with google</button>\n      </div>\n      <ChatContainer v-if=\"showChat\" :messages=\"messages\" @send-message=\"$emit('send-message', $event)\"/>\n    </div>\n    <div class=\"user-hamburger-menu__icon\" @click=\"toggleMenu\" :class=\"{'rotated': show}\">\n      <img src=\"@/assets/hamburger_icon.png\">\n      <div class=\"notification-badge\" v-if=\"showChat && !show && unreadMessages\"/>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {useStore} from \"vuex\";\nimport {logoutUrl} from \"../constants/constants\";\nimport axios from \"axios\";\nimport ChatContainer from \"./hamburgerMenuComponents/ChatContainer\";\n\nexport default {\n  name: \"UserHamburgerMenu\",\n  components: {ChatContainer},\n  props: {\n    showChat: Boolean,\n    messages: Array,\n    disableLogout: Boolean,\n    unreadMessages: Boolean\n  },\n  data(){\n    return{\n      store: useStore(),\n      show: false\n    }\n  },\n  computed: {\n    logged: function(){\n      return this.store.state.logged;\n    },\n    username: function (){\n      return this.store.state.username;\n    }\n  },\n  mounted() {\n    window.addEventListener(\"click\", ()=>{this.show=false});\n  },\n  methods: {\n    toggleMenu(){\n      this.show = !this.show;\n      this.$emit(\"chat-opened\");\n    },\n    redirectToHome(){\n      window.location.href = \"/\";\n    },\n    redirectToGoogle(){\n      let from_location = window.location;\n      window.location.href = '/auth/google?from_location='+from_location;\n    },\n    logout(){\n      axios.get(logoutUrl);\n      this.store.dispatch(\"setLogged\", false);\n      this.store.dispatch(\"setUsername\", null);\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n@import \"../styles/global\";\n\n.user-hamburger-menu__main-panel{\n  position: fixed;\n  top: 0;\n  left: 0;\n  width: fit-content;\n  height: fit-content;\n  display: flex;\n  flex-flow: row;\n  transition: all 0.5s;\n  z-index: 200;\n\n  &.hidden{\n    transform: translateX(calc(-25vw - 1px));\n  }\n\n  .user-hamburger-menu__menu{\n    width: 25vw;\n    height: 100vh;\n    background: linear-gradient(-45deg, $papero-color-dark, $papero-color-light);\n    overflow: hidden;\n    display: flex;\n    flex-flow: column;\n    align-items: center;\n\n    .home-button{\n      height: 25vh;\n    }\n\n    .logged-menu{\n      height: 20vh;\n      display: flex;\n      flex-flow: column;\n      align-items: center;\n      color: white;\n      width: 100%;\n\n      div{\n        width: 100%;\n        display: flex;\n        justify-content: space-evenly;\n        align-items: center;\n        font-size: 2vw;\n        flex-wrap: wrap;\n        margin-bottom: 10px;\n\n        p{\n          white-space: nowrap;\n        }\n      }\n    }\n\n    .not-logged-menu{\n      height: 20vh;\n      display: flex;\n      flex-flow: column;\n      align-items: center;\n      color: white;\n      font-size: 1.5vw;\n\n      p{\n        margin: 10px;\n      }\n    }\n  }\n\n  .user-hamburger-menu__icon{\n    position: relative;\n    margin-top: 15px;\n    margin-left: 15px;\n    width: 5vh;\n    height: 5vh;\n    transition: all 0.5s;\n\n    img{\n      width: 100%;\n      height: 100%;\n    }\n\n    div{\n      position: absolute;\n      right: -1vh;\n      top: 0;\n      width: 2vh;\n      height: 2vh;\n      border-radius: 50%;\n      background-color: red;\n    }\n\n    &.rotated{\n      transform: rotate(90deg);\n    }\n  }\n}\n</style>"],"sourceRoot":""}]}