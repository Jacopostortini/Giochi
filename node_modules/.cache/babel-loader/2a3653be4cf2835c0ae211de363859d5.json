AA,CAAuC,GAAvC,EAAA,WAAA,EAAuC,gBAAA,CAAT,QAAA,CAAA,EAAS,CAAvC,EAAgC,CAAhC,CAEI,E,gBADJ,YAAA,CAA2C,QAA3C,EAA2C,IAA3C,EAA8B,MAA9B,EAAkC,GAAlC,C,EAAkC,C,uBAAR,QAAA,CAAA,E,EAAN,M,CAAc,C,CAC9B,CALN,CAMI,CAbN,CAcI,CAhCN,C","sourcesContent":["<template>\n  <div class=\"pregame-phase__main-panel\">\n    <header class=\"pregame-phase__header\">\n      Scotland Yard\n    </header>\n    <div class=\"pregame-phase__players\">\n      <PlayerLabel v-for=\"player in players\"\n                   :key=\"player.local_id\"\n                   :player=\"player\"\n                   :current-player=\"currentPlayer\"\n                   @kickplayer=\"$emit('kickplayer', $event)\"/>\n    </div>\n    <div class=\"pregame-phase__buttons\">\n      <button v-if=\"currentPlayer\">Change your color</button>\n      <button v-if=\"!currentPlayer\" @click=\"$emit('joingame')\">Join game</button>\n      <button v-if=\"currentPlayer && currentPlayer.is_admin\">Start game</button>\n      <button v-if=\"currentPlayer && currentPlayer.is_admin\">Choose Mister X</button>\n      <button v-if=\"currentPlayer && players.length!==1\" @click=\"$emit('quitgame')\">Quit game</button>\n    </div>\n    <div class=\"pregame-phase__information-panel\">\n      <div class=\"pregame-phase__copy-url\">\n        <label for=\"pregame-phase__url\">Copy the url and share it with your friends!</label>\n        <br>\n        <input id=\"pregame-phase__url\" readonly :value=\"url\">\n        <button v-clipboard:copy=\"url\">Copy</button>\n      </div>\n      <div class=\"pregame-phase__copy-tag\">\n        <label for=\"pregame-phase__tag\">Or send them this tag!</label>\n        <br>\n        <p id=\"pregame-phase__tag\">{{ id }}</p>\n        <button v-clipboard:copy=\"id\">Copy</button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport PlayerLabel from \"./pregamePhaseComponents/PlayerLabel\";\nimport {useRoute} from \"vue-router\";\nexport default {\n  name: \"PregamePhase\",\n  components: {PlayerLabel},\n  props: {\n    players: {\n      type: Array,\n      required: true\n    },\n    currentPlayer: {\n      required: true\n    }\n  },\n  computed: {\n    url: function() {\n      return window.location;\n    },\n    id: function() {\n      return useRoute().params.gameId;\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n@import \"../styles/global\";\n\n.pregame-phase__main-panel{\n  background: linear-gradient(-30deg, $theme-color-light, $theme-color-dark);\n  display: grid;\n  justify-items: center;\n  align-items: center;\n  grid-template-columns: 1fr 1fr auto;\n  grid-template-rows: 20% 50% 30%;\n  grid-template-areas: \"header header header\" \"players players info\" \"footer footer footer\";\n  height: 100%;\n  padding-right: 5%;\n\n  .pregame-phase__header{\n    grid-area: header;\n    color: white;\n    font-weight: bolder;\n    font-size: 2em;\n  }\n\n  .pregame-phase__players{\n    grid-area: players;\n    display: flex;\n    flex-flow: row;\n    flex-wrap: wrap;\n    justify-content: center;\n    color: white;\n    height: 100%;\n    width: 100%;\n    padding: 10%;\n  }\n\n  .pregame-phase__buttons{\n    grid-area: footer;\n    display: flex;\n    flex-flow: row;\n    width: 100%;\n    justify-content: space-evenly;\n\n    button{\n      font-size: 2em;\n      border: none;\n      border-radius: 20px;\n      padding: 2%;\n      color: $theme-color-dark;\n      box-shadow: 1px 1px 5px gray;\n      height: fit-content;\n\n      &:active{\n        transform: translate(1px);\n        box-shadow: none;\n      }\n\n      &:focus{\n        outline: none;\n      }\n    }\n  }\n\n  .pregame-phase__information-panel{\n    grid-area: info;\n    color: white;\n\n    .pregame-phase__copy-url{\n\n      input{\n        margin-top: 10px;\n        margin-right: 10px;\n        width: 300px;\n        color: $theme-color-dark;\n      }\n    }\n\n    .pregame-phase__copy-tag{\n      margin-top: 50px;\n\n      p {\n        display: inline-block;\n        color: $theme-color-dark;\n        background-color: white;\n        padding: 10px;\n        margin-right: 10px;\n      }\n\n      button{\n        display: inline-block;\n      }\n    }\n\n\n  }\n}\n</style>"],"sourceRoot":""}]}