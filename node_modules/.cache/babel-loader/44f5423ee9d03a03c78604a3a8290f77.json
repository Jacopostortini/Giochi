A,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,aAAA,IAAA,QAAA,CAAA,aAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAW9B,GAXN,E,CACE,YAAA,CAKM,KALN,EAAA,UAAA,EAKM,CAJJ,YAAA,CAEM,KAFN,EAEM;AAFD,IAAA,KAAK,EAAC,gBAEL;AAFuB,IAAA,EAAE,EAAE,MAAA,CAAA,IAAA,CAAK,OAAL,GAAY;AAEvC,GAFN,E,CACE,YAAA,CAAqF,IAArF,EAAqF;AAAhF,IAAA,EAAE,EAAE,MAAA,CAAA,IAAA,CAAK,OAAL,GAAY,MAAgE;AAAtD,IAAA,KAAK,EAAA;AAAA,mBAAgB,KAAA,CAAA;AAAhB;AAAiD,GAArF,E,iBAAkE,MAAA,CAAA,IAAA,CAAK,O,CAAvE,EAA8E,EAA9E,EAA8E,CAAA,IAAA,CAA9E,C,CADF,E,CAAA,E,MAAA,CAII,EADJ,YAAA,CAAqD,KAArD,EAAA,UAAA,EAAqD,gBAAA,CAAjB,MAAA,CAAA,IAAA,CAAK,IAAY,CAArD,EAA6C,CAA7C,CACI,CALN,C,EAM4C,KAAA,CAAA,O,kBAA5C,YAAA,CAGM,KAHN,EAAA,UAAA,EAGM,CAFJ,YAAA,CAAkD,QAAlD,EAAkD;AAAzC,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,cAAA,CAAA;AAAA,aAAO,QAAA,CAAA,cAAA,IAAA,QAAA,CAAA,cAAA,OAAA,QAAA,YAAP;AAAA,KAAA,EAAqB,CAAA,MAAA,CAArB,CAAA;AAAoC,GAAlD,EAAqC,MAArC,CAEI,EADJ,YAAA,CAA4C,QAA5C,EAA4C;AAAnC,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,cAAA,CAAA;AAAA,aAAO,QAAA,CAAA,QAAA,IAAA,QAAA,CAAA,QAAA,OAAA,QAAA,YAAP;AAAA,KAAA,EAAe,CAAA,MAAA,CAAf,CAAA;AAA8B,GAA5C,EAA+B,MAA/B,CACI,CAHN,C,kCAPF,C","sourcesContent":["<template>\r\n  <div class=\"game__main-panel\" @click=\"toggleClicked\">\r\n    <div class=\"game__not-clicked-main-panel\">\r\n      <div class=\"name-container\" :id=\"game.game_id+'container'\">\r\n        <h1 :id=\"game.game_id+'name'\" :class=\"{'overflows': overflows}\">{{game.game_id}}</h1>\r\n      </div>\r\n      <div class=\"game__date-container\">{{game.date}}</div>\r\n    </div>\r\n    <div class=\"game__clicked-main-panel\" v-if=\"clicked\">\r\n      <button @click.stop=\"redirectToGame\">Join</button>\r\n      <button @click.stop=\"quitGame\">Quit</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {useRouter} from \"vue-router\";\r\nimport axios from \"axios\";\r\nimport {urls} from \"../../constants/constants\";\r\n\r\nexport default {\r\n  name: \"Game\",\r\n  props: {\r\n    game: {\r\n      type: Object,\r\n      required: true\r\n    }\r\n  },\r\n  data(){\r\n    return {\r\n      overflows: false,\r\n      router: useRouter(),\r\n      clicked: false\r\n    }\r\n  },\r\n  methods: {\r\n    toggleClicked(){\r\n      this.clicked = !this.clicked;\r\n    },\r\n    redirectToGame(){\r\n      this.router\r\n          .push({\r\n                name: \"Game\",\r\n                params: {\r\n                  gameId: this.game.game_id\r\n                }\r\n              });\r\n    },\r\n    quitGame(){\r\n      axios\r\n          .post(urls.quitGameUrl, {game_id: this.game.game_id})\r\n          .then(()=>{\r\n            this.router.go(0);\r\n          });\r\n    }\r\n  },\r\n  mounted() {\r\n    let name = document.getElementById(this.game.game_id+\"name\");\r\n    let container = document.getElementById(this.game.game_id+\"container\")\r\n    if(name){\r\n      this.overflows =  container.clientWidth<name.scrollWidth;\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import \"../../styles/global\";\r\n\r\n.game__main-panel{\r\n  margin: 2%;\r\n  border: 1px solid white;\r\n  color: white;\r\n  position: relative;\r\n\r\n  .game__not-clicked-main-panel{\r\n    animation: fade-in 0.5s;\r\n    display: grid;\r\n    grid-template-columns: 60% 40%;\r\n    align-items: center;\r\n    justify-content: center;\r\n    transition: all 0.5s;\r\n\r\n    .name-container{\r\n      overflow: hidden;\r\n      margin: 0 5% 0 5%;\r\n      text-align: left;\r\n      white-space: nowrap;\r\n\r\n      h1{\r\n        font-size: 1.5vw;\r\n        font-weight: normal;\r\n        width: fit-content;\r\n        @media (max-width: 700px) {\r\n          font-size: 5vw;\r\n        }\r\n      }\r\n\r\n      .overflows:hover{\r\n        animation: scroll-text 5s linear infinite;\r\n      }\r\n    }\r\n\r\n    &:hover{\r\n      background-color: white;\r\n      color: $theme-color-dark;\r\n    }\r\n\r\n    .game__date-container{\r\n      font-size: 1.5vw;\r\n      text-align: left;\r\n      @media (max-width: 700px) {\r\n        font-size: 5vw;\r\n      }\r\n    }\r\n  }\r\n\r\n  .game__clicked-main-panel{\r\n    animation: fade-in 0.5s;\r\n    position: absolute;\r\n    top: 0;\r\n    left: 0;\r\n    width: 100%;\r\n    height: 100%;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: space-evenly;\r\n    background-color: white;\r\n\r\n    button{\r\n      border: 2px solid black;\r\n      color: black;\r\n      font-size: 3vh;\r\n\r\n      &:hover{\r\n        background-color: black;\r\n        color: white;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n@keyframes scroll-text {\r\n  0%{\r\n    transform: translate(0, 0);\r\n  }\r\n  100%{\r\n    transform: translate(-100%, 0);\r\n  }\r\n}\r\n\r\n@keyframes fade-in {\r\n  0%{\r\n    opacity: 0;\r\n  }\r\n  100%{\r\n    opacity: 1;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}