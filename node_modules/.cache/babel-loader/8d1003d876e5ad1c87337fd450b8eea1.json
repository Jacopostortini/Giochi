;AACb,WAAK,OAAL,GAAe,CAAC,KAAK,OAArB;AACD,KAHM;AAIP,IAAA,cAJO,4BAIS;AACd,WAAK,MAAL,CACK,IADL,CACU;AACA,QAAA,IAAI,EAAE,MADN;AAEA,QAAA,MAAM,EAAE;AACN,UAAA,MAAM,EAAE,KAAK,IAAL,CAAU;AADZ;AAFR,OADV;AAOD,KAZM;AAaP,IAAA,QAbO,sBAaG;AAAA;;AACR,MAAA,KAAI,CACC,IADL,CACU,IAAI,CAAC,WADf,EAC4B;AAAC,QAAA,OAAO,EAAE,KAAK,IAAL,CAAU;AAApB,OAD5B,EAEK,IAFL,CAEU,YAAI;AACR,QAAA,KAAI,CAAC,MAAL,CAAY,EAAZ,CAAe,CAAf;AACD,OAJL;AAKF;AAnBO,GAfI;AAoCb,EAAA,OApCa,qBAoCH;AACR,QAAI,IAAG,GAAI,QAAQ,CAAC,cAAT,CAAwB,KAAK,IAAL,CAAU,OAAV,GAAkB,MAA1C,CAAX;AACA,QAAI,SAAQ,GAAI,QAAQ,CAAC,cAAT,CAAwB,KAAK,IAAL,CAAU,OAAV,GAAkB,WAA1C,CAAhB;;AACA,QAAG,IAAH,EAAQ;AACN,WAAK,SAAL,GAAkB,SAAS,CAAC,WAAV,GAAsB,IAAI,CAAC,WAA7C;AACF;AACF;AA1Ca,CAAf","sourcesContent":["<template>\n  <div class=\"game__main-panel\" @click=\"toggleClicked\">\n    <div class=\"game__not-clicked-main-panel\">\n      <div class=\"name-container\" :id=\"game.game_id+'container'\">\n        <h1 :id=\"game.game_id+'name'\" :class=\"{'overflows': overflows}\">{{game.game_id.toUpperCase()}}</h1>\n      </div>\n      <div class=\"game__date-container\">{{game.date}}</div>\n    </div>\n    <div class=\"game__clicked-main-panel\" v-if=\"clicked\">\n      <button @click.stop=\"redirectToGame\">Join</button>\n      <button @click.stop=\"quitGame\">Quit</button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {useRouter} from \"vue-router\";\nimport axios from \"axios\";\nimport {urls} from \"../../constants/constants\";\n\nexport default {\n  name: \"Game\",\n  props: {\n    game: {\n      type: Object,\n      required: true\n    }\n  },\n  data(){\n    return {\n      overflows: false,\n      router: useRouter(),\n      clicked: false\n    }\n  },\n  methods: {\n    toggleClicked(){\n      this.clicked = !this.clicked;\n    },\n    redirectToGame(){\n      this.router\n          .push({\n                name: \"Game\",\n                params: {\n                  gameId: this.game.game_id\n                }\n              });\n    },\n    quitGame(){\n      axios\n          .post(urls.quitGameUrl, {game_id: this.game.game_id})\n          .then(()=>{\n            this.router.go(0);\n          });\n    }\n  },\n  mounted() {\n    let name = document.getElementById(this.game.game_id+\"name\");\n    let container = document.getElementById(this.game.game_id+\"container\")\n    if(name){\n      this.overflows =  container.clientWidth<name.scrollWidth;\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n@import \"../../styles/global\";\n\n.game__main-panel{\n  margin: 2%;\n  border: 1px solid white;\n  color: white;\n  position: relative;\n\n  .game__not-clicked-main-panel{\n    animation: fade-in 0.5s;\n    display: grid;\n    grid-template-columns: 60% 40%;\n    align-items: center;\n    justify-content: center;\n    transition: all 0.5s;\n\n    .name-container{\n      overflow: hidden;\n      margin: 0 5% 0 5%;\n      text-align: left;\n      white-space: nowrap;\n\n      h1{\n        font-size: 1.5vw;\n        font-weight: normal;\n        width: fit-content;\n        @media (max-width: 700px) {\n          font-size: 5vw;\n        }\n      }\n\n      .overflows:hover{\n        animation: scroll-text 5s linear infinite;\n      }\n    }\n\n    &:hover{\n      background-color: white;\n      color: $theme-color-dark;\n    }\n\n    .game__date-container{\n      font-size: 1.5vw;\n      text-align: left;\n      @media (max-width: 700px) {\n        font-size: 5vw;\n      }\n    }\n  }\n\n  .game__clicked-main-panel{\n    animation: fade-in 0.5s;\n    position: absolute;\n    top: 0;\n    left: 0;\n    width: 100%;\n    height: 100%;\n    display: flex;\n    align-items: center;\n    justify-content: space-evenly;\n    background-color: white;\n\n    button{\n      border: 2px solid black;\n      color: black;\n      font-size: 3vh;\n\n      &:hover{\n        background-color: black;\n        color: white;\n      }\n    }\n  }\n}\n\n@keyframes scroll-text {\n  0%{\n    transform: translate(0, 0);\n  }\n  100%{\n    transform: translate(-100%, 0);\n  }\n}\n\n@keyframes fade-in {\n  0%{\n    opacity: 0;\n  }\n  100%{\n    opacity: 1;\n  }\n}\n</style>\n"],"sourceRoot":""}]}