ername: player.username, isAdmin: player.user_id===adminId}\"\r\n                   :color=\"player.color\"\r\n                   :is-admin=\"isAdmin\"\r\n                   :is-you=\"player.user_id===user.user_id\"/>\r\n    </div>\r\n    <div class=\"pregame-phase__buttons\">\r\n      <button v-if=\"players.includes(user)\">Change your color</button>\r\n      <button v-if=\"!players.includes(user)\">Join game</button>\r\n      <button v-if=\"isAdmin\">Start game</button>\r\n      <button v-if=\"isAdmin\">Choose Mister X</button>\r\n    </div>\r\n    <div class=\"pregame-phase__information-panel\">\r\n      <div class=\"pregame-phase__copy-url\">\r\n        <label for=\"pregame-phase__url\">Copy the url and share it!</label>\r\n        <br>\r\n        <input id=\"pregame-phase__url\" readonly :value=\"url\">\r\n        <button v-clipboard:copy=\"url\">Copy</button>\r\n      </div>\r\n      <div class=\"pregame-phase__copy-tag\">\r\n        <label for=\"pregame-phase__tag\">Or send them this tag</label>\r\n        <br>\r\n        <p id=\"pregame-phase__tag\">{{ tag }}</p>\r\n        <button v-clipboard:copy=\"tag\">Copy</button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport PlayerLabel from \"./pregamePhaseComponents/PlayerLabel\";\r\nimport {useRoute} from \"vue-router\";\r\nexport default {\r\n  name: \"PregamePhase\",\r\n  components: {PlayerLabel},\r\n  props: {\r\n    players: {\r\n      type: Array,\r\n      required: true\r\n    },\r\n    user: {\r\n      type: Object,\r\n      required: true\r\n    },\r\n    adminId: {\r\n      type: String,\r\n      required: true\r\n    }\r\n  },\r\n  computed: {\r\n    isAdmin: function() {\r\n      return this.user ? this.user.user_id===this.adminId : false;\r\n    },\r\n    url: function() {\r\n      return window.location;\r\n    },\r\n    tag: function() {\r\n      return useRoute().params.gameTag;\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import \"../../styles/global.scss\";\r\n\r\n.pregame-phase__main-panel{\r\n  background: linear-gradient($theme-color-dark, $theme-color-light);\r\n  display: grid;\r\n  justify-items: center;\r\n  grid-template-columns: 1fr 1fr auto;\r\n  grid-template-rows: 100px auto auto;\r\n  height: 100%;\r\n  padding-top: 20px;\r\n\r\n  .pregame-phase__header{\r\n    grid-column-start: 1;\r\n    grid-column-end: 4;\r\n    color: white;\r\n    font-weight: bolder;\r\n    font-size: 2em;\r\n  }\r\n\r\n  .pregame-phase__players{\r\n    display: flex;\r\n    flex-flow: row;\r\n    flex-wrap: wrap;\r\n    justify-content: center;\r\n    grid-column-start: 1;\r\n    grid-column-end: 3;\r\n    grid-row-start: 2;\r\n    color: white;\r\n    padding-left: 100px;\r\n    padding-right: 100px;\r\n  }\r\n\r\n  .pregame-phase__buttons{\r\n    grid-column-start: 1;\r\n    grid-column-end: 4;\r\n    grid-row-start: 3;\r\n    display: flex;\r\n    flex-flow: row;\r\n\r\n    button{\r\n      margin: 20px;\r\n      font-size: 2em;\r\n      border: none;\r\n      border-radius: 20px;\r\n      padding: 20px;\r\n      color: $theme-color-dark;\r\n      box-shadow: 1px 1px 5px gray;\r\n      height: fit-content;\r\n\r\n      &:active{\r\n        transform: translate(1px);\r\n        box-shadow: none;\r\n      }\r\n\r\n      &:focus{\r\n        outline: none;\r\n      }\r\n    }\r\n  }\r\n\r\n  .pregame-phase__information-panel{\r\n    grid-column-start: 3;\r\n    grid-row-start: 2;\r\n    color: white;\r\n    margin-right: 50px;\r\n\r\n    .pregame-phase__copy-url{\r\n\r\n      input{\r\n        margin-top: 10px;\r\n        margin-right: 10px;\r\n        width: 300px;\r\n        color: $theme-color-dark;\r\n      }\r\n    }\r\n\r\n    .pregame-phase__copy-tag{\r\n      margin-top: 50px;\r\n\r\n      p {\r\n        display: inline-block;\r\n        color: $theme-color-dark;\r\n        background-color: white;\r\n        padding: 10px;\r\n        margin-right: 10px;\r\n      }\r\n\r\n      button{\r\n        display: inline-block;\r\n      }\r\n    }\r\n\r\n\r\n  }\r\n}\r\n</style>"],"sourceRoot":""}]}