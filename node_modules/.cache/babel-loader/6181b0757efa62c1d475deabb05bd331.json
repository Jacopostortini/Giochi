KAAK,EAAC;;;AAKN,EAAA,KAAK,EAAC;;;AAON,EAAA,KAAK,EAAC;;;AAIN,EAAA,KAAK,EAAC;;;;;;;;;;uBAjBb,YAAA,CAoBM,KApBN,EAAA,UAAA,EAoBM,CAnBJ,YAAA,CAGM,KAHN,EAAA,UAAA,EAGM,CAFU,MAAA,CAAA,aAAA,IAAiB,MAAA,CAAA,aAAA,CAAc,Q,kBAA7C,YAAA,CAAiH,QAAjH,EAAiH;UAAA;AAAzD,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,IAAA,CAAA,KAAA,CAAK,cAAL,EAAK,IAAL,CAAF;AAAA,KAAA;AAAoD,GAAjH,EAA4F,cAA5F,C,mBACA,YAAA,CAAqE,wBAAA,CAA3B,KAAA,CAAA,OAA2B,CAArE,EAAiD;UAAA;AAAjC,IAAA,OAAK,EAAE,QAAA,CAAA;AAA0B,GAAjD,E;qBAAmD;AAAA,aAAS,C,kCAAN,KAAA,CAAA,G,GAAG,C,CAAG,CAAT;AAAA,K;;GAAnD,E,CAAA,E,WAAA,C,CACI,CAHN,CAmBI,EAdJ,YAAA,CAKM,KALN,EAAA,UAAA,EAKM,E,kBAJJ,YAAA,CAGiE,SAHjE,EAGiE,IAHjE,EAGiE,WAAA,CAHnC,MAAA,CAAA,OAGmC,EAH5B,UAAjB,MAAiB,EAAX;yBAA1B,YAAA,CAGiE,sBAHjE,EAGiE;AAFnD,MAAA,GAAG,EAAE,MAAM,CAAC,QAEuC;AADnD,MAAA,MAAM,EAAE,MAC2C;AAAnD,oBAAY,MAAM,CAAC,QAAP,KAAkB,MAAA,CAAA,IAAA,CAAK;AAAgB,KAHjE,E,IAAA,E,CAAA,E,wBAAA,C;GAGiE,CAHjE,E,GAAA,CAII,EALN,CAcI,EAPJ,YAAA,CAEM,KAFN,EAAA,UAAA,EAEM,CADJ,YAAA,CAAmD,uBAAnD,EAAmD;AAApC,sBAAgB,MAAA,CAAA,IAAA,CAAK;AAAe,GAAnD,E,IAAA,E,CAAA,E,kBAAA,CACI,CAFN,CAOI,EAHJ,YAAA,CAEM,KAFN,EAAA,UAAA,EAEM,CADJ,YAAA,CAA8E,QAA9E,EAA8E,IAA9E,EAA8E,gBAAA,CAApE,MAAA,CAAA,IAAA,CAAK,UAAL,GAAe,eAAf,GAAe,qBAAqD,CAA9E,EAAyB,CAAzB,CACI,CAFN,CAGI,CApBN,C","sourcesContent":["<template>\n  <div class=\"post-game-sidebar__main-panel\">\n    <div class=\"post-game-sidebar__title\">\n      <button v-if=\"currentPlayer && currentPlayer.is_admin\" @click=\"$emit('restart-game', true)\">Restart game</button>\n      <button v-else @click=\"backToLobby\" v-is=\"element\">{{ msg }}</button>\n    </div>\n\n    <div class=\"post-game-sidebar__players\">\n      <PlayerLabel v-for=\"player in players\"\n                   :key=\"player.local_id\"\n                   :player=\"player\"\n                   :is-playing=\"player.local_id===game.playingPlayer\"/>\n    </div>\n\n    <div class=\"mister-x-table__wrapper\">\n      <MisterXTable :mister-x-moves=\"game.misterXMoves\"/>\n    </div>\n\n    <div class=\"winning-badge\">\n      <strong>{{game.misterXWon ? \"Mister X won!\" : \"The detectives won!\"}}</strong>\n    </div>\n  </div>\n</template>\n\n<script>\nimport PlayerLabel from \"../gamePhaseComponents/PlayerLabel\";\nimport MisterXTable from \"../postGamePhaseComponents/MisterXTable\";\nimport LoadingBackToLobby from \"./LoadingBackToLobby\";\n\nexport default {\n  name: \"PostGameSideBar\",\n  components: {MisterXTable, PlayerLabel, LoadingBackToLobby},\n  props: {\n    players: Array,\n    game: Object,\n    currentPlayer: Object,\n    gameRestarted: Boolean\n  },\n  data(){\n    return {\n      wantToGoBack: false,\n      element: \"button\",\n      msg: \"Back to lobby\"\n    }\n  },\n  methods: {\n    backToLobby(){\n      if(this.gameRestarted) this.$emit(\"restart-game\", false);\n      else {\n        this.wantToGoBack = true;\n        this.msg = null\n        this.element = \"LoadingBackToLobby\"\n      }\n    }\n  },\n  watch: {\n    gameRestarted: function(val){\n      if((this.wantToGoBack || (this.currentPlayer && this.currentPlayer.is_admin)) && val)\n        this.$emit(\"restart-game\", false);\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n\n.post-game-sidebar__main-panel {\n  grid-area: sidebar;\n  overflow-y: scroll;\n  display: grid;\n  grid-template-rows: auto auto 1fr auto;\n  grid-template-columns: 1fr;\n  grid-template-areas: \"title\" \"labels\" \"mister-x-table\" \"winning-badge\";\n\n  justify-content: left;\n  padding-top: 10px;\n\n  .post-game-sidebar__title {\n    grid-area: title;\n    text-align: center;\n    margin: 10px;\n\n    button{\n      font-size: 2vw;\n      @media (max-width: 500px) {\n        font-size: 4vw;\n      }\n    }\n  }\n\n  .post-game-sidebar__players {\n    grid-area: labels;\n    width: 100%;\n    display: grid;\n    grid-template-columns: 1fr 1fr 1fr;\n    align-items: center;\n  }\n\n  .mister-x-table__wrapper{\n    grid-area: mister-x-table;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    align-self: start;\n  }\n\n  .winning-badge{\n    grid-area: winning-badge;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n  }\n}\n\n</style>"],"sourceRoot":""}]}