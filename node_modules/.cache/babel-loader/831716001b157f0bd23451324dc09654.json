A;;AACR,IAAA,MAAM,CAAC,gBAAP,CAAwB,OAAxB,EAAiC,YAAI;AAAC,MAAA,KAAI,CAAC,IAAL,GAAU,KAAV;AAAgB,KAAtD;AACD,GAxBY;AAyBb,EAAA,OAAO,EAAE;AACP,IAAA,cADO,4BACS;AACd,MAAA,MAAM,CAAC,QAAP,CAAgB,IAAhB,GAAuB,GAAvB;AACD,KAHM;AAIP,IAAA,gBAJO,8BAIW;AAChB,UAAI,aAAY,GAAI,MAAM,CAAC,QAA3B;AACA,MAAA,MAAM,CAAC,QAAP,CAAgB,IAAhB,GAAuB,gCAA8B,aAArD;AACD,KAPM;AAQP,IAAA,MARO,oBAQC;AACN,MAAA,KAAK,CAAC,GAAN,CAAU,SAAV;AACA,WAAK,KAAL,CAAW,QAAX,CAAoB,WAApB,EAAiC,KAAjC;AACA,WAAK,KAAL,CAAW,QAAX,CAAoB,aAApB,EAAmC,IAAnC;AACF;AAZO;AAzBI,CAAf","sourcesContent":["<template>\r\n  <div class=\"user-hamburger-menu__main-panel\" @click.stop=\"\" :class=\"{'hidden': !show}\">\r\n    <div class=\"user-hamburger-menu__menu\">\r\n      <img class=\"home-button\" src=\"@/assets/logo.png\" @click=\"redirectToHome\">\r\n      <div class=\"logged-menu\" v-if=\"username\">\r\n        <div>\r\n          <p>Username: <strong>{{username}}</strong></p>\r\n          <button @click=\"logout\" v-if=\"!disableLogout\">Logout</button>\r\n        </div>\r\n        <button class=\"procede-with-google\" v-if=\"!logged\" @click=\"redirectToGoogle\">Activate sync with google</button>\r\n      </div>\r\n      <div class=\"not-logged-menu\" v-else>\r\n        <p>You are currently not logged in</p>\r\n        <button class=\"procede-with-google\" @click=\"redirectToGoogle\">Sign in now with google</button>\r\n      </div>\r\n      <ChatContainer v-if=\"showChat\" :messages=\"messages\" @send-message=\"$emit('send-message', $event)\"/>\r\n    </div>\r\n    <div class=\"user-hamburger-menu__icon\" @click=\"show=!show\" :class=\"{'rotated': show}\">\r\n      <img src=\"@/assets/hamburger_icon.png\">\r\n      <div class=\"notification-badge\"/>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {useStore} from \"vuex\";\r\nimport {logoutUrl} from \"../constants/constants\";\r\nimport axios from \"axios\";\r\nimport ChatContainer from \"./hamburgerMenuComponents/ChatContainer\";\r\n\r\nexport default {\r\n  name: \"UserHamburgerMenu\",\r\n  components: {ChatContainer},\r\n  props: {\r\n    showChat: Boolean,\r\n    messages: Array,\r\n    disableLogout: Boolean\r\n  },\r\n  data(){\r\n    return{\r\n      store: useStore(),\r\n      show: false\r\n    }\r\n  },\r\n  computed: {\r\n    logged: function(){\r\n      return this.store.state.logged;\r\n    },\r\n    username: function (){\r\n      return this.store.state.username===null ? null : decodeURIComponent(this.store.state.username);\r\n    }\r\n  },\r\n  mounted() {\r\n    window.addEventListener(\"click\", ()=>{this.show=false});\r\n  },\r\n  methods: {\r\n    redirectToHome(){\r\n      window.location.href = \"/\";\r\n    },\r\n    redirectToGoogle(){\r\n      let from_location = window.location;\r\n      window.location.href = '/auth/google?from_location='+from_location;\r\n    },\r\n    logout(){\r\n      axios.get(logoutUrl);\r\n      this.store.dispatch(\"setLogged\", false);\r\n      this.store.dispatch(\"setUsername\", null);\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import \"../styles/global\";\r\n\r\n.user-hamburger-menu__main-panel{\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: fit-content;\r\n  height: fit-content;\r\n  display: flex;\r\n  flex-flow: row;\r\n  transition: all 0.5s;\r\n  z-index: 200;\r\n\r\n  &.hidden{\r\n    transform: translateX(calc(-25vw - 1px));\r\n  }\r\n\r\n  .user-hamburger-menu__menu{\r\n    width: 25vw;\r\n    height: 100vh;\r\n    background: linear-gradient(-45deg, $papero-color-dark, $papero-color-light);\r\n    overflow: hidden;\r\n    display: flex;\r\n    flex-flow: column;\r\n    align-items: center;\r\n\r\n    .home-button{\r\n      height: 25vh;\r\n    }\r\n\r\n    .logged-menu{\r\n      height: 20vh;\r\n      display: flex;\r\n      flex-flow: column;\r\n      align-items: center;\r\n      color: white;\r\n      width: 100%;\r\n\r\n      div{\r\n        width: 100%;\r\n        display: flex;\r\n        justify-content: space-evenly;\r\n        align-items: center;\r\n        font-size: 2vw;\r\n        flex-wrap: wrap;\r\n        margin-bottom: 10px;\r\n\r\n        p{\r\n          white-space: nowrap;\r\n        }\r\n      }\r\n    }\r\n\r\n    .not-logged-menu{\r\n      height: 20vh;\r\n      display: flex;\r\n      flex-flow: column;\r\n      align-items: center;\r\n      color: white;\r\n      font-size: 1.5vw;\r\n\r\n      p{\r\n        margin: 10px;\r\n      }\r\n    }\r\n  }\r\n\r\n  .user-hamburger-menu__icon{\r\n    position: relative;\r\n    margin-top: 15px;\r\n    margin-left: 15px;\r\n    width: 5vh;\r\n    height: 5vh;\r\n    transition: all 0.5s;\r\n\r\n    img{\r\n      width: 100%;\r\n      height: 100%;\r\n    }\r\n\r\n    div{\r\n      position: absolute;\r\n      right: -1vh;\r\n      top: 0;\r\n      width: 2vh;\r\n      height: 2vh;\r\n      border-radius: 50%;\r\n      background-color: red;\r\n    }\r\n\r\n    &.rotated{\r\n      transform: rotate(90deg);\r\n    }\r\n  }\r\n}\r\n</style>"],"sourceRoot":""}]}