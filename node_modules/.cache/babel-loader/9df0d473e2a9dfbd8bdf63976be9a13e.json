ns\">\n      <img :src=\"require('@/assets/pawns/pawn_'+n+'.png')\" v-for=\"n in colorsAvailable\" :key=\"n\" @click=\"$emit('changecolor', n)\">\n    </div>\n  </div>\n</template>\n\n<script>\nimport { colors } from \"../../constants/constants\";\n\nexport default {\n  name: \"ChangeColorPopup\",\n  props: {\n    players: {\n      type: Array,\n      required: true\n    }\n  },\n  computed: {\n    colorsAvailable: function (){\n      let colorsUnavailable = [];\n      for (let i=0; i<this.players.length; i++){\n        colorsUnavailable.push(this.players[i].color);\n      }\n      let colorsAvailable = [];\n      for (let i=0; i<colors; i++){\n        if(!colorsUnavailable.includes(i)) colorsAvailable.push(i);\n      }\n      return colorsAvailable;\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n@import \"../../styles/global\";\n\n.change-popup__main-panel{\n  width: 50%;\n  height: fit-content;\n  position: relative;\n  color: white;\n  text-align: center;\n  display: flex;\n  flex-flow: column;\n  justify-content: space-between;\n\n  .change-popup__table-panel {\n    position: absolute;\n    bottom: 0;\n    left: 0;\n    width: 100%;\n    height: 12%;\n    background-color: white;\n    transform: skewX(80deg);\n  }\n\n  .change-popup__available-pawns{\n    z-index: 1000;\n    padding-bottom: 2%;\n\n    img{\n      width: 10%;\n      margin-right: 5%;\n      transition: transform 0.25s;\n\n      &:hover{\n        transform: scale(1.1);\n      }\n    }\n  }\n\n}\n\n</style>"],"sourceRoot":""}]}